{
  "name": "myvue",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "dev:prod": "vite --mode production",
    "build:prod": "pnpm run sitemap && pnpm exec vite build --mode production",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview --mode production",
    "build-only": "vite build",
    "start": "pnpm run dev",
    "sitemap": "node updateSitemap.js",
    "lint-staged": "lint-staged",
    "check:type": "vue-tsc --build --noEmit --force",
    "check:prettier": "prettier --check .",
    "check": "eslint .",
    "lint": "eslint . --fix",
    "lint:prettier": "prettier . --write",
    "lint:commitlint": "commitlint --edit $1",
    "lint:diff": "git add . && pnpm exec lint-staged",
    "commit": "pnpm run lint:diff && cz",
    "push": "git push Gitee && git push origin",
    "prepare": "husky"
  },
  "lint-staged": {
    "**/*.{cjs,mjs,js,jsx,tsx,ts,less,md,json}": [
      "pnpm exec eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@highlightjs/vue-plugin": "github:highlightjs/vue-plugin",
    "@vueuse/motion": "^2.2.6",
    "@wangeditor/editor": "^5.1.23",
    "@wangeditor/editor-for-vue": "^5.1.12",
    "axios": "^1.7.9",
    "bignumber.js": "^9.1.2",
    "dayjs": "^1.11.13",
    "element-plus": "^2.9.7",
    "file-saver": "^2.0.5",
    "gridstack": "^11.2.0",
    "highlight.js": "^11.11.1",
    "jwt-decode": "^4.0.0",
    "mitt": "^2.1.0",
    "motion-v": "0.10.0-beta.1",
    "pinia": "^2.3.0",
    "pinia-plugin-persistedstate": "^4.2.0",
    "socket.io-client": "^4.8.1",
    "spark-md5": "^3.0.2",
    "vue": "^3.5.13",
    "vue-intersect": "^3.0.0",
    "vue-router": "^4.5.0",
    "vue3-swatches": "^1.2.4",
    "wc-flow-layout": "^0.0.5",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@commitlint/types": "^19.5.0",
    "@eslint/js": "^9.19.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^20.17.12",
    "@types/spark-md5": "^3.0.5",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "@vitejs/plugin-legacy": "^5.4.3",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/language-server": "^2.2.0",
    "@vue/tsconfig": "^0.5.1",
    "cz-git": "^1.11.0",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "npm-run-all2": "^6.2.6",
    "prettier": "^3.4.2",
    "sass": "^1.83.4",
    "sass-loader": "^16.0.4",
    "sitemap": "^8.0.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.22.0",
    "unplugin-auto-import": "^0.17.8",
    "unplugin-element-plus": "^0.9.1",
    "unplugin-icons": "^0.20.2",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.4.11",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-html-minify": "^1.0.0",
    "vite-plugin-vue-devtools": "^7.7.0",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vite-svg-loader": "^5.1.0",
    "vitepress": "^1.5.0",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.2.0"
  }
}
